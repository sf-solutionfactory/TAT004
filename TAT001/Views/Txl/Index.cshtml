@model IEnumerable<TAT001.Entities.TAX_LAND>
@{
    ViewBag.pagina_r = 502;
    ViewBag.carpeta_r = 500;
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col s12 right-align">
        @Html.ActionLink("Agregar", "Create", "Txl", new { @class = "btn-small" })
        @using (Html.BeginForm("Descargar", "Txl", FormMethod.Post, new { @style = "display:initial;" }))
        {
            <button id="btnExport" name="excel" class="btn-small" type="submit" value="Excel"></button>
        }
    </div>
</div>
<div class="col 12 s12 ">
    <div class="card-panel">
        <div class="row">
            <table class="table mdl-data-table striped" id="table" style="width:100%;">
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.SOCIEDAD_ID)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.PAI.LAND)
                    </th>
                    <th></th>
                </tr>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.SOCIEDAD_ID)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.PAI.LAND)
                            </td>
                            <td>
                                <a data-form-method="post" href="@Url.Action("Delete", "Txl",new { sid = item.SOCIEDAD_ID,pid=item.PAI.LAND})">
                                    <i class='material-icons red-text'>delete</i>
                                </a>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>