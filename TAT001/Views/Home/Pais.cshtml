@model IEnumerable<TAT001.Entities.PAI>
@{
    //ViewBag.Title = "Seleccionar país";
    ViewBag.pagina_r = 102;
    ViewBag.carpeta_r = 100;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<form runat="server">
    <div class="row">
        <div class="col s12 m6 l4">
            <div class="row">
            </div>
        </div>
        <div class=" col s12 m12 l8">
            <div class="row">
                @foreach (var item in Model)
                {
                    <div class="col s6 m4 l2 center">
                        <div class="row">
                            <input id="btn_@item.LAND.ToLower()" type="image" src="~/images/flags/1x1/@item.IMAGE" class="circle" style="height:100px;width:100px;" onclick="location.href='@Url.Action("Index", "Home", new {pais=item.LAND})';return false;" />
                        </div>
                    </div>
                }
                @*<div class="col s12 m6 l4">
                        <div style="height:100px;width:100px;">
                            <input id="btn_mx" type="image" src="~/images/flags/1x1/mx.svg" class="circle" style="height:100px;width:100px;" onclick="location.href='@Url.Action("Index", "Home", new {pais="mx"})';return false;" />
                        </div>
                    </div>
                    <div class="col s12 m6 l4">
                        <input id="btn_pr" type="image" src="~/images/flags/1x1/pr.svg" class="circle" style="height:100px;width:100px;" onclick="location.href='@Url.Action("Index", "Home", new {pais="pr"})';return false;" />
                    </div>
                    <div class="col s12 m6 l4">
                        <input id="btn_ec" type="image" src="~/images/flags/1x1/ec.svg" class="circle" style="height:100px;width:100px;" onclick="location.href='@Url.Action("Index", "Home", new {pais="ec"})';return false;" />
                    </div>*@
            </div>

            <div class="row">
                @foreach (var item in ViewBag.delegados)
                {
                    <div class="col s6 m4 l2 center">
                        <div class="row">
                            <input id="btn_@item.LAND.ToLower()" type="image" src="~/images/flags/1x1/@item.IMAGE" class="circle" style="height:100px;width:100px;" onclick="location.href='@Url.Action("Index", "Home", new {pais=item.LAND})';return false;" />
                        </div>
                    </div>
